# RUN: toyc-lab %s -emit=mlir -opt 2>&1 | FileCheck %s

def main() {
  var a<2, 3> = [[1, 2, 3], [4, 5, 6]];
  var b<2, 3> = [1, 2, 3, 4, 5, 6];
  var c = a-b;
  print(c);
}

# CHECK-LABEL: func @main()
# CHECK-NEXT:    [[VAL_0:%.*]] = toy.constant dense<{{\[\[}}1.000000e+00, 2.000000e+00, 3.000000e+00], [4.000000e+00, 5.000000e+00, 6.000000e+00]]> : tensor<2x3xf64>
# CHECK-NEXT:    [[VAL_1:%.*]] = toy.sub [[VAL_0]], [[VAL_0]] : tensor<2x3xf64>
# CHECK-NEXT:    toy.print [[VAL_1]] : tensor<2x3xf64>
# CHECK-NEXT:    toy.return
